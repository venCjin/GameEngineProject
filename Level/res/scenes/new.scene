-AddSystem SimplePlayerSystem
-AddSystem CollisionSystem

-AddShader StaticPBR res/shaders/Basic.glsl

//GROUND
-AddMaterial GroundBasic Basic 1 res/models/Environment/Ground/Textures/Ground_Albedo.png
+SceneGameObject
    +Transform
    +end
    -BoxCollider 1000.0 0.1 1000.0 1
    -Model res/models/Environment/Ground/Ground.obj
    -Material GroundBasic
+end

//CUBE
-AddMaterial BrickBasic Basic 1 res/textures/test/Bricks.jpg
+SceneGameObject
    +Transform
        +world
            -translate 1.0 0.5 0.0
        +end
    +end
    -Gizmo 0 255 0 Cube 1.0 1.0 1.0
    -Gizmo 255 0 0 Line 0 0 0 10 0 0
    -BoxCollider 1.0 1.0 1.0 0
    -SimplePlayer
    -Model res/models/Primitives/Cube.obj
    -Material BrickBasic
+end

//ROCK_5
-AddMaterial RocksBasic Basic 1 res/models/Environment/Rocks/Textures/Rocks_Albedo.png
+SceneGameObject
    +Transform
        +world
            -translate 0.0 0.64 0.0
        +end
    +end
    -Gizmo 0 255 0 Cube 1.4 1.3 1.4
    -BoxCollider 1.4 1.3 1.4 0
    -Model res/models/Environment/Rocks/Rock5.obj
    -Material RocksBasic
+end
